<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />
				<property name="password" value="${password}" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="wpbn/pgis/entity/xml/UserMapper.xml" />
		<mapper resource="wpbn/pgis/entity/xml/StudentMapper.xml" />
		<mapper resource="wpbn/pgis/entity/xml/CourseMapper.xml" />
		<mapper resource="wpbn/pgis/entity/xml/InstructorMapper.xml" />
		<mapper resource="wpbn/pgis/entity/xml/ClassMapper.xml" />
		
	</mappers>
</configuration>
